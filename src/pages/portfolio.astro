---
import Layout from "../layouts/Layout.astro"
import "../styles/global.css";
import Navbar from "../components/Navbar.astro"
import Footer from "../components/Footer.astro"


---

<Layout title="Fagerli">
	
<Navbar />
<main class="main">
    <ul>
        <li><a id="sudokulink" href="/projects/sudoku">Sudoku</a></li>
      </ul>
</main>
<script>
    function forceReload(event) {
        event.preventDefault();
        window.location.href = event.currentTarget.href;
    }

    // Select the anchor element and add the event listener
    document.querySelector('#sudokulink').addEventListener('click', forceReload);
</script>
<Footer />
</Layout>

